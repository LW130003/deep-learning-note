    "    tmp_key = 'bad:'+'-'.join(tmp_bad)+',good:'+'-'.join(tmp_good)\n",
    "    sim_analyzer = WaferMapSimilarity(window_RoI=window_RoI, resize=resize)\n",
    "    step_seq_df = query_step_sequence_jdbc(tmp_good[0])\n",
    "    sim_analyzer.calc_similarity_score(w_good=(tmp_good[0], tmp_good[1]), w_bad=(tmp_bad[0], tmp_bad[1]), step_sequence=step_seq_df, pretrained_model=pretrained_model)\n",
    "    score_dict_bg[tmp_key] = sim_analyzer.similarity_score_sorted\n",
    "    counter += 1\n",
    "    "
