    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "# sns.set(style=\"whitegrid\")\n",
    "sns.set()\n",
    "fig = plt.figure(figsize=(30, 15))\n",
    "ax = sns.boxplot(x=\"step\", y=\"score\", data=all_result_df, color=\"seagreen\")\n",
    "sns.boxplot(x=\"step\", y=\"score\", data=all_result_df_bg, color=\"red\")\n",
    "ax.grid(True)\n",
    "plt.ylim(0.2, 1.05)\n",
    "fig.autofmt_xdate()\n",
    "# plt.xticks(list(range(-2, 71)), fontsize=8)\n",
    "plt.xlabel('WIS step', fontsize=20)\n",
    "plt.ylabel('Similarity score', fontsize=20)\n",
    "plt.title('Boxplot of similarity scores in each WIS step', fontsize=20)\n",
    "plt.show()\n",
    "fig.savefig('results/N18A-gg-bg-boxplot.svg')"
